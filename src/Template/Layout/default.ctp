	<script type="text/javascript">
		function sendEmailViaChat() {
			var email = $("#email").val();
			if (email != "") {
				$crisp.push(["set", "user:email", email]);
			}
			else {
				Materialize.toast('Please enter email address', 4000);
				return;
			}
			var message = $("#message").val();
			if (message != "") {
				$crisp.push(["do", "message:send", ["text", message]]);
				$crisp.push(["do", "chat:open"]);
				$("#email").val("");
				$("#message").val("");
			}
			else {
				Materialize.toast('Please enter message', 4000);
				return;
			}			
		}
		function sendEmailViaChatContactForm() {
			var emailcontact = $("#emailcontact").val();
			if (emailcontact != "") {
				$crisp.push(["set", "user:email", emailcontact]);
			}
			else {
				Materialize.toast('Please enter email address', 4000);
				return;
			}
			var message = $("#messagecontact").val();
			if (message != "") {
				$crisp.push(["do", "message:send", ["text", message]]);
				$crisp.push(["do", "chat:open"]);
				$("#emailcontact").val("");
				$("#messagecontact").val("");
			}
			else {
				Materialize.toast('Please enter message', 4000);
				return;
			}			
		}
	</script>
	
